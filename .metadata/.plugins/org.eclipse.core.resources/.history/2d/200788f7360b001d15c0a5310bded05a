#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1=1,num2,answer;
	printf("please enter 1 number");
	fflush(stdout);
	scanf("%d",&num2);
	answer=num1*num2;
	printf("%d x %d = %d\n",num1,num2,answer);
	answer=num2+answer;
	printf("%d x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("3 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("4 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("5 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("6 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("7 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("8 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("9 x %d = %d\n",num2,answer);
	answer=num2+answer;
	printf("10 x %d = %d\n",num2,answer);
	return EXIT_SUCCESS;
}
