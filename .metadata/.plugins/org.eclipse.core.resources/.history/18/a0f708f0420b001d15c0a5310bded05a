#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a,b,c;
	printf("enter two numbers\n");
	scanf("%d%d",&a,&b);
	c=a+b;
	printf("result is %d",c);
	return EXIT_SUCCESS;
}
