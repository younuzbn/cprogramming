#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2,choice;
	printf("enter two numbers");
	fflush(stdout);
	scanf("%d%d",&num1,&num2);
	printf("select an option\n")
	printf("addition\n")
	printf("subtraction\n")
	printf("multiplication\n")
	printf("division")
	scanf("%d",&choice);
	if(choice==1)
	return EXIT_SUCCESS;
}
